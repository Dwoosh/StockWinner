<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="712.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.GraphDialogController">
    <children>
        <LineChart fx:id="lineChart" layoutX="9.0" layoutY="11.0" prefHeight="478.0" prefWidth="695.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" side="LEFT" />
            </yAxis>
        </LineChart>
        <Button fx:id="backButton" layoutX="233.0" layoutY="491.0" mnemonicParsing="false" onAction="#handleGoingBack" text="Back" />
        <Button fx:id="resetButton" layoutX="383.0" layoutY="491.0" mnemonicParsing="false" onAction="#handleReset" text="Reset" />
    </children>
</AnchorPane>
